<!DOCTYPE html>
<html ng-app="catsvsdogs"><head>
    <meta charset="utf-8">
    <title>Cats vs Dogs Result</title>
    <base href="/index.ejs">
    <link rel="shortcut icon" type="image/x-icon" href="img/icono.ico">
    <meta name="viewport" content="width=device-width, initial-scale = 1.0">
    <meta name="keywords" content="docker-compose, docker, stack">
    <meta name="author" content="Docker">
    <link rel="stylesheet" href="style.css">
    <meta http-equiv="refresh" content="5">
  </head>
  <body ng-controller="statsCtrl">
     <div id="background-stats">
       <div id="background-stats-1" style="width:<%-div_izq%>%;">
       </div><!--
      --><div id="background-stats-2" style="width:<%-div_der%>%;">
      </div>
    </div>
    <div id="content-container">
      <div id="content-container-center">
        <div id="choice">
          <div class="choice cats">
            <div class="label">Cats</div>
            <div class="stat"><%= cats_percent %>%</div>
          </div>
          <div class="divider"></div>
          <div class="choice dogs">
            <div class="label">Dogs</div>
            <div class="stat"><%= dogs_percent %>%</div>
          </div>
        </div>
      </div>
    </div>
    <div id="result">
    <span><%= total %></span>
    </div>
    <!-- <script src="socket.io.js"></script>
    <script src="angular.min.js"></script> -->
    <!--<script src="app.js"></script> -->
</body></html>